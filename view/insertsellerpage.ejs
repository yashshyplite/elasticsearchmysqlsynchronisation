<html>
    <%- include('./partials/head');%>
<body>
    <%- include('./partials/sellernav');%>
    <br>
    <div class="container">
            <form method="post" action='/insertseller'>
                <div class='mb-3'>
                    <label for="sellername" class="form-label">Seller Name</label>
                    <input type="text" class="form-control" id="sellername" name="sellername">
                </div>
                <div class='mb-3'>
                    <label for="mobile" class="form-label">Mobile Number</label>
                    <input type="number" class="form-control" id="mobile" name="mobileNo">
                </div>
                <div class='mb-3'>
                    <label for="email" class="form-label">E-Mail</label>
                    <input type="email" class="form-control" id="email" name="email">
                </div>
                <div class='mb-3'>
                    <label for="address" class="form-label">Address</label>
                    <input type="text" class="form-control" id="address" name="address">
                </div>
                <button type="submit" class="btn btn-primary">  Insert Seller </button>
            </form>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>